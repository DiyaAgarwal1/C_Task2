// print index of the last 1 present in the array.If no one is present in the array print -1

#include <iostream>
using namespace std;
int main()
{
    int n,a[n],ind=0,s=0,i;
    cout<<"Enter the length of array : ";
    cin>>n;
    cout<<"Enter the elements of array : ";
    for(i=0;i<n;i++)
    {
     cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        if(a[i]==1)
        {
          ind=i;
          s=1;
        }
    }
    if(s==0)
    cout<<-1<<endl;
    else
    cout<<ind<<endl;
return 0;
}
