//program to print x and recursively x=x-5 till x=0 and then recursively x=x+5 till x=x back

#include <iostream>
  using namespace std;
  int rec(int n){
    if(n<=0)
    {
      cout<<n<<" ";
      return 0;
    }
    cout<<n<<" ";
    rec(n-5);
    cout<<n<<" ";
    return 0;
  }
  int main()
  {
    int n;
    cout<<"Enter the number : ";
    cin>>n;
    rec(n);
    return 0;
  }
