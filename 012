// multiply elements of the array

#include <iostream>
  using namespace std;
  int main()
  {
    int n,arr[n],i,product=1;
    cout<<"Enter the length of array : ";
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for (i=0;i<n;i++)
    {
        product*=arr[i];
    }
    cout<<product<<endl;
     
    return 0;
  }
