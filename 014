//from an array print all the even elements in the first line and all odd elements in the second line.

#include <iostream>
using namespace std;
  
  int main()
  {
    int n,j=0,k=0,i;
    cout<<"Enter the size of array : ";
    cin>>n;
    int arr[n],even[n],odd[n];
    for(i=0;i<n;i++)
    {
      cin>>arr[i];
      if(arr[i]%2 == 0)
      {
        even[j] = arr[i];
        j++;
      }
      else
      {
        odd[k] = arr[i];
        k++;
      }
    }
    for(i=0;i<j;i++)
    cout<<even[i]<<" ";
    cout<<endl;
    for(i=0;i<k;i++)
    cout<<odd[i]<<" ";
    return 0;
  }
