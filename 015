//from an array print minimum and maximum elements separated by space.

#include <iostream>
using namespace std;
  
  int main()
  {
      int n,arr[n],i;
      cout<<"Enter the length of array : ";
      cin>>n;
      cout<<"Enter the elements of array : ";
      for(i=0;i<n;i++)
      {
        cin>>arr[i];
      }
      int min=arr[0],max=arr[0];
      for(int i=1;i<n;i++)
      {
        if(arr[i]<min)
         min=arr[i];
        if(arr[i]>max)
         max=arr[i];
      }
      cout<<min<<" "<<max<<endl;
    return 0;
  }
